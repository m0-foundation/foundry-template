{
  "plugins": ["prettier-plugin-solidity"],
  "overrides": [
    {
      "files": "*.sol",
      "options": {
        "bracketSpacing": true,
        "compiler": "0.8.26",
        "parser": "slang",
        "printWidth": 120,
        "singleQuote": false,
        "tabWidth": 4,
        "useTabs": false
      }
    }
  ]
}
